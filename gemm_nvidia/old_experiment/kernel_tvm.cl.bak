#include "settings.h"

__kernel void rcl__kernel0(__global float* restrict A, __global float* restrict B, __global float* restrict C) {
  for (int y = 0; y < BW; ++y) {
    C[((get_group_id(0) * TS + get_local_id(0)) * BW) + y] = 0.0f;

    for (int k = 0; k < AW; ++k) {
      C[((get_group_id(0) * TS + get_local_id(0)) * BW) + y] = 
      C[((get_group_id(0) * TS + get_local_id(0)) * BW) + y] + A[(get_group_id(0) * 8 + get_local_id(0)) * AW + k] * B[y + k * BW];
    }
  }
}
